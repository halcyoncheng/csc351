    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "MIMode": "lldb"
        },
        {
            "name": "Debug first",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/test",
            "args": [
                "10"
            ],
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "env": {},
            "terminal": "external"
        }
    ]
}